{
  "name": "vue-show-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prettier": "prettier --cache --check **/**/*.{ts,js,vue}",
    "prettier:fix": "prettier --cache --write **/**/*.{ts,js,vue}",
    "stylelint": "stylelint **/*.{scss,vue} --cache",
    "stylelint:fix": "npx stylelint **/*.{scss,vue} --fix --cache",
    "eslint": "eslint src --cache --ext .js,.jsx,.ts,.tsx,.vue",
    "eslint:fix": "eslint src --fix --cache --ext .js,.jsx,.ts,.tsx,.vue"
  },
  "dependencies": {
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/bootstrap": "^5.2.6",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.0",
    "sass": "1.64.2",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  }
}
